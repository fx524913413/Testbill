package com.ruijie.spl.billingEngine.billing.tariffStrategies;
dialect  "mvel"

import com.ruijie.spl.billingEngine.billing.entity.*;
import java.util.List;

rule "ARule"
    when
        $person:PersonEntity_Ab($age:age>18)
    then
        System.out.println($person.getFirstName()+ " "+$person.getLastName()+ " "+$person.getAge());
    then[firstCondition]
end

rule "BRule"
    when
        $person:PersonEntity_Ab($age:age<=18)
    then
        System.out.println($age);
end

rule "Kobe's Name"
    when
        $list:List()
        $person:PersonEntity_Ab($firstName:firstName,$lastname:lastName,$age:age>20) from $list
        if(true) break[first]
        if(false) do[second]
    then
        System.out.println("I'm "+$firstName+" "+$lastname+", I'm "+$age);
    then[first]
        System.out.println("111");
    then[second]
        System.out.println("222");
end
